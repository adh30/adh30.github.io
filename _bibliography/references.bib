@article{RN45771,
   author = {Treibel, T. A. and Kozor, R. and Fontana, M. and Torlasco, C. and Reant, P. and Badiani, S. and Espinoza, M. and Yap, J. and Diez, J. and Hughes, A. D. and Lloyd, G. and Moon, J. C.},
   title = {Sex Dimorphism in the Myocardial Response to Aortic Stenosis},
