@article{RN34552,
   author = {Williams, B. and Lacy, P. S. and Thom, S. M. and Cruickshank, K. and Stanton, A. and Collier, D. and Hughes, A. D. and Thurston, H. and O'Rourke, M.},
   title = {Differential impact of blood pressure-lowering drugs on central aortic pressure and clinical outcomes: principal results of the Conduit Artery Function Evaluation (CAFE) study},
