@article{RN29853,
   author = {Eriksen, A. and Tillin, T. and O'Connor, L. and Brage, S. and Hughes, A. and Mayet, J. and McKeigue, P. and Whincup, P. and Chaturvedi, N. and Forouhi, N. G.},
