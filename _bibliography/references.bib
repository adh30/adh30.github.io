   url = {http://www.ncbi.nlm.nih.gov/pubmed/17083062},
   year = {2006},
   type = {Journal Article}
}

@article{RN40752,
   author = {Garcha, R. S. and Sever, P. S. and Hughes, A. D.},
   title = {The action of amlodipine on human subcutaneous resistance arteries studied in vitro},
