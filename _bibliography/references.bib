@article{RN44347,
   author = {Ferenczi, E. A. and Asaria, P. and Hughes, A. D. and Chaturvedi, N. and Francis, D. P.},
   title = {Can a statin neutralize the cardiovascular risk of unhealthy dietary choices?},
   journal = {Am J Cardiol},
   volume = {106},
