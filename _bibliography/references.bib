   title = {Carotid Reservoir Pressure Decrease After Prolonged Head Down Tilt Bed Rest in Young Healthy Subjects Is Associated With Reduction in Left Ventricular Ejection Time and Diastolic Length},
   journal = {Front Physiol},
   volume = {13},
   pages = {866045},
   note = {Palombo, Carlo
