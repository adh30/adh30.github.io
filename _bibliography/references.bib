@article{RN30543,
   author = {Hedegaard, E. R. and Nielsen, B. D. and Kun, A. and Hughes, A. D. and Kroigaard, C. and Mogensen, S. and Matchkov, V. V. and Frobert, O. and Simonsen, U.},
   title = {KV 7 channels are involved in hypoxia-induced vasodilatation of porcine coronary arteries},
