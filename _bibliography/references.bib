@article{RN59325,
   author = {Chiesa, S. T. and Norris, T. and Garfield, V. and Richards, M. and Hughes, A. D.},
   title = {Early-life cumulative exposure to excess bodyweight and midlife cognitive function: longitudinal analysis in three British birth cohorts},
   journal = {Lancet Healthy Longev},
   volume = {5},
