@article{RN60350,
   author = {Hendrick, E. and Jamieson, A. and Chiesa, S. T. and Hughes, A. D. and Jones, S.},
   title = {A short review of application of near-infrared spectroscopy (NIRS) for the assessment of microvascular post-occlusive reactive hyperaemia (PORH) in skeletal muscle},
   journal = {Front Physiol},
   volume = {15},
