@article{RN41542,
   author = {Knoops, P. G. and Biglino, G. and Hughes, A. D. and Parker, K. H. and Xu, L. and Schievano, S. and Torii, R.},
   title = {A Mock Circulatory System Incorporating a Compliant 3D-Printed Anatomical Model to Investigate Pulmonary Hemodynamics},
   journal = {Artif Organs},
   note = {Knoops, Paul G M
Biglino, Giovanni
