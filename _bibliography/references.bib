@article{RN30197,
   author = {Wimalasundera, R. C. and Wijetunge, S. and Thom, S. M. and Regan, L. and Hughes, A. D.},
   title = {Impaired recovery of intracellular calcium and force after activation in isolated myometrial and subcutaneous resistance arteries from women with preeclampsia},
