@article{RN61810,
   author = {Topriceanu, C. C. and Gong, X. and Shah, M. and Shiwani, H. and Eminson, K. and Atilola, G. O. and Jephcote, C. and Adams, K. and Blangiardo, M. and Moon, J. C. and Hughes, A. D. and Gulliver, J. and Rowlands, A. V. and Chaturvedi, N. and O'Regan, D. P. and Hansell, A. L. and Captur, G.},
