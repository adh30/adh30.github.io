}

@article{RN34720,
   author = {Tyberg, J. V. and Davies, J. E. and Wang, Z. and Whitelaw, W. A. and Flewitt, J. A. and Shrive, N. G. and Francis, D. P. and Hughes, A. D. and Parker, K. H. and Wang, J. J.},
