@article{RN53780,
   author = {Seraphim, A. and Knott, K. D. and Menacho, K. and Augusto, J. B. and Davies, R. and Pierce, I. and Joy, G. and Bhuva, A. N. and Xue, H. and Treibel, T. A. and Cooper, J. A. and Petersen, S. E. and Fontana, M. and Hughes, A. D. and Moon, J. C. and Manisty, C. and Kellman, P.},
   title = {Prognostic Value of Pulmonary Transit Time and Pulmonary Blood Volume Estimation Using Myocardial Perfusion CMR},
