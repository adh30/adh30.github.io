}

@article{RN51777,
   author = {Al Saikhan, L. and Alobaida, M. and Bhuva, A. and Chaturvedi, N. and Heasman, J. and Hughes, A. D. and Jones, S. and Eastwood, S. and Manisty, C. and March, K. and Ghosh, A. K. and Mayet, J. and Oguntade, A. and Tillin, T. and Williams, S. and Wright, A. and Park, C.},
   title = {Imaging Protocol, Feasibility, and Reproducibility of Cardiovascular Phenotyping in a Large Tri-Ethnic Population-Based Study of Older People: The Southall and Brent Revisited (SABRE) Study},
   journal = {Front Cardiovasc Med},
   volume = {7},
   pages = {591946},
   note = {Al Saikhan, Lamia
Alobaida, Muath
Bhuva, Anish
