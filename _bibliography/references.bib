@article{RN55219,
   author = {Charalambous, C. and Moon, J. C. and Holly, J. M. P. and Chaturvedi, N. and Hughes, A. D. and Captur, G.},
   title = {Declining Levels and Bioavailability of IGF-I in Cardiovascular Aging Associate With QT Prolongation-Results From the 1946 British Birth Cohort},
   journal = {Front Cardiovasc Med},
   volume = {9},
   pages = {863988},
   note = {Charalambous, Christos
Moon, James C
Holly, Jeff M P
