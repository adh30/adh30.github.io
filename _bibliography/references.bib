@article{RN56423,
   author = {Wels, J. and Wielgoszewska, B. and Moltrecht, B. and Booth, C. and Green, M. J. and Hamilton, O. K. and Demou, E. and Di Gessa, G. and Huggins, C. and Zhu, J. and Santorelli, G. and Silverwood, R. J. and Kopasker, D. and Shaw, R. J. and Hughes, A. and Patalay, P. and Steves, C. and Chaturvedi, N. and Porteous, D. J. and Rhead, R. and Katikireddi, S. V. and Ploubidis, G. B.},
   title = {Home working and social and mental wellbeing at different stages of the COVID-19 pandemic in the UK: Evidence from 7 longitudinal population surveys},
