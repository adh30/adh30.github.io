@article{RN51781,
   author = {Kerrison, P. D. and Innes, M. and Macleod, A. and McCormick, E. and Elbourne, P. D. and Stanley, M. S. and Hughes, A. D. and Kelly, M. S.},
   title = {Comparing the effectiveness of twine- and binder-seeding in the Laminariales species Alaria esculenta and Saccharina latissima},
