@article{RN30506,
   author = {Baruah, R. and Giannoni, A. and Willson, K. and Manisty, C. H. and Mebrate, Y. and Kyriacou, A. and Yadav, H. and Unsworth, B. and Sutton, R. and Mayet, J. and Hughes, A. D. and Francis, D. P.},
   title = {Novel cardiac pacemaker-based human model of periodic breathing to develop real-time, pre-emptive technology for carbon dioxide stabilisation},
