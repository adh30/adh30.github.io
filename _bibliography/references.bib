@article{RN61795,
   author = {Birnie, K. and Howe, L. D. and Jones, T. and Madley-Dowd, P. and Martin, F. Z. and Forbes, H. and Redaniel, M. T. and Cornish, R. and Magnus, M. C. and Davies, N. M. and Tilling, K. and Hughes, A. D. and Lawlor, D. A. and Fraser, A.},
