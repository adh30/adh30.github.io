@article{RN41543,
   author = {Sofat, R. and Cooper, J. A. and Kumari, M. and Casas, J. P. and Mitchell, J. P. and Acharya, J. and Thom, S. and Hughes, A. D. and Humphries, S. E. and Hingorani, A. D.},
   title = {Circulating Apolipoprotein E Concentration and Cardiovascular Disease Risk: Meta-analysis of Results from Three Studies},
