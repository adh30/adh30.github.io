@article{RN30412,
   author = {Jones, R. and Hardy, R. and Sattar, N. and Deanfield, J. E. and Hughes, A. and Kuh, D. and Murray, E. T. and Whincup, P. H. and Thomas, C.},
   title = {Novel coronary heart disease risk factors at 60-64 years and life course socioeconomic position: The 1946 British birth cohort},
