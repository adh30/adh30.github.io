@article{RN40401,
   author = {Khamis, R. Y. and Hughes, A. D. and Caga-Anan, M. and Chang, C. L. and Boyle, J. J. and Kojima, C. and Welsh, P. and Sattar, N. and Johns, M. and Sever, P. and Mayet, J. and Haskard, D. O.},
