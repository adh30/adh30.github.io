@article{RN60546,
   author = {van Dinther, M. and Bennett, J. and Thornton, G. D. and Voorter, P. H. M. and Ezponda Casajus, A. and Hughes, A. and Captur, G. and Holtackers, R. J. and Members, Crucial Consortium Clinical and Staals, J. and Backes, W. H. and Bastarika, G. and Jones, E. A. V. and Gonzalez, A. and van Oostenbrugge, R. J. and Treibel, T. A.},
