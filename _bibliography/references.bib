@article{RN60146,
   author = {Aizawa, K. and Jordan, A. N. and Gooding, K. M. and Llewellyn, D. J. and Mawson, D. M. and Casanova, F. and Gates, P. E. and Adingupu, D. D. and Elyas, S. and Hope, S. V. and Gilchrist, M. and Strain, W. D. and Clark, C. E. and Bellenger, N. G. and Sharp, A. S. P. and Parker, K. H. and Hughes, A. D. and Shore, A. C.},
   title = {Aortic reservoir-excess pressure parameters are associated with worse cognitive function in people with untreated stage II/III hypertension},
