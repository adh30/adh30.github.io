@article{RN31855,
   author = {McGoldrick, R. B. and Kingsbury, M. and Turner, M. A. and Sheridan, D. J. and Hughes, A. D.},
   title = {Left ventricular hypertrophy induced by aortic banding impairs relaxation of isolated coronary arteries},
