@article{RN45383,
   author = {Kyriacou, A. and Rajkumar, C. A. and Pabari, P. A. and Sohaib, S. M. A. and Willson, K. and Peters, N. S. and Lim, P. B. and Kanagaratnam, P. and Hughes, A. D. and Mayet, J. and Whinnett, Z. I. and Francis, D. P.},
   title = {Distinct impacts of heart rate and right atrial-pacing on left atrial mechanical activation and optimal AV delay in CRT},
   journal = {Pacing Clin Electrophysiol},
   note = {1540-8159
Kyriacou, Andreas
Rajkumar, Christopher A
ORCID: http://orcid.org/0000-0002-0086-1113
