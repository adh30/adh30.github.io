   author = {Hadjiloizou, N. and Davies, J. E. and Malik, I. S. and Aguado-Sierra, J. and Willson, K. and Foale, R. A. and Parker, K. H. and Hughes, A. D. and Francis, D. P. and Mayet, J.},
   title = {Differences in cardiac microcirculatory wave patterns between the proximal left mainstem and proximal right coronary artery},
