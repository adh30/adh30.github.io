@article{RN51778,
   author = {Park, C. M. and Hughes, A. D. and Henein, M. Y. and Khir, A. W.},
   title = {Mechanisms of Aortic Flow Deceleration and the Effect of Wave Reflection on Left Ventricular Function},
   journal = {Front Physiol},
   volume = {11},
