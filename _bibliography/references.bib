@article{RN29108,
   author = {Schultz, M. G. and Hughes, A. D. and Davies, J. E. and Sharman, J. E.},
   title = {Associations and clinical relevance of aortic-brachial artery stiffness mismatch, aortic reservoir function and central pressure augmentation},
   journal = {Am J Physiol Heart Circ Physiol},
   pages = {ajpheart 00317 2015},
   note = {Schultz, Martin G
Hughes, Alun D
Davies, Justin E
Sharman, James E
ENG
2015/08/16 06:00
Am J Physiol Heart Circ Physiol. 2015 Aug 14:ajpheart.00317.2015. doi: 10.1152/ajpheart.00317.2015.},
   abstract = {BACKGROUND: Central augmentation pressure (AP) and index (AIx) predict cardiovascular events and mortality, but underlying physiological mechanisms remain disputed. Whilst traditionally believed to relate to wave-reflections arising from proximal arterial impedance (and stiffness) mismatching, recent evidence suggests aortic reservoir function may be a more dominant contributor to AP and AIx. Our aim was therefore to determine relationships between aortic-brachial stiffness mismatching, AP, AIx, aortic reservoir function and end-organ disease. METHODS: Aortic (aPWV) and brachial (bPWV) pulse wave velocity were measured in 359 individuals (aged 61+/-9, 49% male). Central AP, AIx and aortic reservoir indices were derived from radial tonometry. Participants were stratified by positive (bPWV>aPWV), negligible (bPWV approximately aPWV) or negative stiffness mismatch (bPWV<aPWV). Left-ventricular mass index (LVMI) was measured by 2D-echocardiography. RESULTS: Central AP and AIx were higher with negative stiffness mismatch vs. negligible or positive stiffness mismatch (11+/-6 vs. 10+/-6 vs. 8+/-6mmHg, p<0.001 and 24+/-10 vs. 24+/-11 vs. 21+/-13%, p=0.042). Stiffness mismatch (bPWV-aPWV) was negatively associated with AP (r=-0.18,p=0.001), but not AIx (r=-0.06,p=0.27). Aortic reservoir pressure strongly correlated to AP (r=0.81,p<0.001) and AIx (r=0.62,p<0.001) independent of age, sex, heart rate, mean arterial pressure and height (standardized beta=0.61 and 0.12,p</=0.001). Aortic reservoir pressure independently predicted abnormal LVMI (beta=0.13, p=0.024). CONCLUSION: Positive aortic-brachial stiffness mismatch does not result in higher AP or AIx. Aortic reservoir function, rather than discrete wave-reflection from proximal arterial stiffness mismatching provides a better model description of AP and AIx, and also has clinical relevance as evidenced by an independent association of aortic reservoir pressure with LVMI.},
   keywords = {Aorta
aortic stiffness
augmentation index
reservoir},
   ISSN = {1522-1539 (Electronic)
0363-6135 (Linking)},
   DOI = {10.1152/ajpheart.00317.2015},
   url = {http://www.ncbi.nlm.nih.gov/pubmed/26276816
http://ajpheart.physiology.org/content/ajpheart/early/2015/08/10/ajpheart.00317.2015.full.pdf},
   year = {2015},
   type = {Journal Article}
}

