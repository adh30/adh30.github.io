@article{RN52246,
   author = {Bann, D. and Villadsen, A. and Maddock, J. and Hughes, A. and Ploubidis, G. B. and Silverwood, R. and Patalay, P.},
   title = {Changes in the behavioural determinants of health during the COVID-19 pandemic: gender, socioeconomic and ethnic inequalities in five British cohort studies},
   journal = {J Epidemiol Community Health},
   note = {Bann, David
