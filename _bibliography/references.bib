@article{RN61794,
   author = {Thornton, G. D. and McKenna, M. and Bennett, J. and Hughes, A. and Gonzalez, A. and Khanji, M. Y. and Cavalcante, J. L. and Lloyd, G. and Moon, J. C. and Bhattacharyya, S. and Treibel, T. A.},
   title = {Myocardial Remodelling in Aortic Regurgitation: Time to Think Beyond Volumes and Function?},
   journal = {Eur Heart J Cardiovasc Imaging},
